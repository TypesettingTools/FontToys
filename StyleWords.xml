<?xml version="1.0" encoding="utf-8"?>
<styleWords>
    <!-- nuke stuff -->
    <word match="[fF]ont [fF]amily" replaceString=""/>
    
    Font Family
    <!-- keep Regular/Normal styles up here so properties can be overwritten by more specific styles -->
    <word weight="400" fsSelection="64" separate="1">Regular</word>
    <word weight="400" width="5" fsSelection="64" separate="1">Normal</word>

    <!-- standard styl names -->
    <word weight="100" separate="1">UltraThin</word>
    <word weight="100" separate="1">Thin</word>
    <word weight="100" separate="1">UltraLight</word>
    <word weight="200" separate="1">ExtraLight</word>
    <word weight="300" separate="1">Light</word>
    <word weight="400" separate="1">Book</word>
    <word weight="500" separate="1">Medium</word>
    <word weight="600" separate="1" replaceString="SemiBold" match="Semi[Bb]old">SemiBold</word>
    <word weight="600" separate="1">DemiBold</word>
    <word weight="700" fsSelection="32" match="(?&lt;!Semi|Demi|Extra|X)Bold">Bold</word>
    <word weight="800" separate="1">ExtraBold</word>
    <word weight="900" separate="1">Heavy</word>
    <word weight="900" match="(?&lt;!Ultra|Extra)Black">Black</word>
    <word weight="900" match="Ultra(?![Bb]lack)">Ultra</word>
    <word weight="900" separate="1">UltraBlack</word>
    <word weight="1000">Fat</word>
    <word weight="1000" separate="1">ExtraBlack</word>
    <word width="1" separate="1">UltraCondensed</word>
    <word width="2" separate="1">ExtraCondensed</word>
    <word width="3" match="(?&lt;!Ultra|Extra|Semi)Condensed">Condensed</word>
    <word width="3" separate="1" >Narrow</word>
    <word width="3">Compressed</word>
    <word width="4" separate="1">SemiCondensed</word>
    <word width="6" separate="1">SemiExpanded</word>
    <word width="7" match="(?&lt;!Ultra|Extra|Semi|Super)Expanded">Expanded</word>
    <word width="8" separate="1">ExtraExpanded</word>
    <word width="8" separate="1">SuperExpanded</word>
    <word width="9" separate="1">UltraExpanded</word>
    <word fsSelection="1" separate="1">Italic</word>
    <word fsSelection="2" separate="1">Underscore</word>
    <word fsSelection="4">Negative</word>
    <word fsSelection="8" separate="1">Outlined</word>
    <word fsSelection="8" match="Outline(?!d)">Outline</word>
    <word fsSelection="8">Hollow</word>
    <word fsSelection="16" separate="1">Strikeout</word>
    <word fsSelection="512" separate="1">Oblique</word>
    
    <!-- Japanese style names -->
    
    <word weight="100" separate="1">W1</word>
    <word weight="200" separate="1">W2</word>
    <word weight="300" separate="1">W3</word>
    <word weight="400" separate="1">W4</word>
    <word weight="500" separate="1">W5</word>
    <word weight="600" separate="1">W6</word>
    <word weight="700" fsSelection="32" separate="1">W7</word>
    <word weight="800" separate="1">W8</word>
    <word weight="900" separate="1">W9</word>

    <!-- stupid abbreviations -->
    
    <word weight="200" replaceString="ExtraLight" separate="1" caseSensitive="1">ExLt</word>
    <word weight="300" replaceString="Light" match="(?&lt;!Ex)Lt" caseSensitive="1">Lt</word>
    <word weight="500" replaceString="Medium" match="Med(?!ium)" caseSensitive="1">Med</word>
    <word weight="600" replaceString="DemiBold" match="Demi(?![Bb]old)" caseSensitive="1">Demi</word>
    <word weight="700" replaceString="Bold" fsSelection="32" match="Bol(?!d)" caseSensitive="1">Bol</word>
    <word weight="700" replaceString="Bold" fsSelection="32" match="(?&lt;!Ex)Bld" caseSensitive="1">Bld</word>
    <word weight="900" replaceString="Black" separate="1" caseSensitive="1">Blk</word>
    <word replaceString="Oblique" fsSelection="512" separate="1" match="Oblq?(?!ique)" caseSensitive="1">Oblq</word>
    <word replaceString="Italic" fsSelection="1" match="Ital?(?!l?ic)" caseSensitive="1">Ita</word>
    <word replaceString="Italic" fsSelection="1" match="It(?!\w)" caseSensitive="1" onlyLast="1">It</word>
    <word width="3" replaceString="Compressed" caseSensitive="1">Com</word>
    <word width="3" replaceString="Condensed" match="Cond(?!ensed)" caseSensitive="1">Cond</word>
    <word width="3" replaceString="Condensed" caseSensitive="1">Cd</word>
    <word fsSelection="8" replaceString="Outline" caseSensitive="1">Otl</word>

    <!-- more stupid abbreviations with no relation to font metrics -->
    <word replaceString="Broken" separate="1" caseSensitive="1">Brk</word>
    <word replaceString="Inkblotted" separate="1">Ikbl</word>
    <word replaceString="Slanted" separate="1" caseSensitive="1">Slt</word>
    <word replaceString="SmallCaps" separate="1" caseSensitive="1">Smc</word>
    <word replaceString="SmallCaps" caseSensitive="1" match="SC(?![\p{Lu}\p{Lt}])">SC</word>

    <!-- acceptable abbreviations -->
    <word caseSensitive="1">OSF</word> <!-- Old Style Figures -->
    <word match="P[Rr]([56])(N?)" caseSensitive="1" replaceString="PR$1$2" separate="1">Pr#N</word> <!-- Adobe-Japan1-5/6 w/ JIS version -->
    
    <!-- nonstandard alternates -->
    <word width="8" separate="1" replaceString="ExtraExpanded">Extra Wide</word>
    <word width="7" separate="1" replaceString="Expanded" match="(?&lt;!Extra)Wide">Wide</word>
    <word weight="800" separate="1" replaceString="ExtraBold" caseSensitive="1">XBold</word>
    <word fsSelection="1" separate="1">Cursive</word>
    <word match="S(mall)?[\s-_]*[Cc]aps" separate="1" replaceString="SmallCaps" caseSensitive="1">Small Caps</word>
    <word fsSelection="512" separate="1">Leftleaning</word>


    <!-- might not need those anymore 
    <word weight="700" fsSelection="33" replaceString="Bold Italic">BoldItalic</word>
    <word weight="400" fsSelection="64" width="3" replaceString="Condensed">CondensedRegular</word>
    <word weight="400" fsSelection="64" width="7" replaceString="Expanded">ExpandedRegular</word>
    <word weight="500" fsSelection="1" replaceString="Medium Italic">MediumItalic</word>
    <word weight="400" fsSelection="64" width="8" replaceString="UltraExpanded">UltraExpandedRegular</word>
    <word weight="400" fsSelection="64" width="8" replaceString="UltraExpanded">UltraexpandedRegula</word>
    --> 

